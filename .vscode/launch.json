    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main",
            "type": "node",
            "request": "launch",
            "args": ["src/cli.ts"],
            "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Clone",
            "type": "node",
            "request": "launch",
            "args": ["src/run-clone-uptime.ts"],
            "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
    
}